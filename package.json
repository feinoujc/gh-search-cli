{
  "name": "gh-search-cli",
  "description": "github.com search cli",
  "version": "2.4.0",
  "author": "feinoujc @feinoujc",
  "bin": {
    "ghs": "./bin/run"
  },
  "bugs": "https://github.com/feinoujc/gh-search-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.4.4",
    "@oclif/config": "^1.3.60",
    "@oclif/parser": "^3.8.4",
    "@oclif/plugin-help": "2",
    "chalk": "^2.3.2",
    "cli-ux": "^5.2.1",
    "fs-extra": "^8.1.0",
    "git-user-name": "^2.0.0",
    "parse-link-header": "^1.0.1",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^8.0.0",
    "@types/git-user-name": "^2.0.0",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.1.2",
    "@types/node": "^12.7.7",
    "@types/open": "6.2.1",
    "@types/parse-link-header": "^1.0.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/sinon": "^7.0.13",
    "chai": "^4.1.2",
    "cross-env": "^6.0.0",
    "globby": "^10.0.1",
    "mocha": "^6.2.0",
    "nock": "^11.3.5",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/feinoujc/gh-search-cli",
  "keywords": [
    "oclif",
    "github",
    "search",
    "api",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ghs",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/auth"
    }
  },
  "repository": "feinoujc/gh-search-cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "prettier:check": "prettier --check **/*.{js,json,ts,yaml,yml}",
    "prettier": "prettier --write **/*.{js,json,ts,yaml,yml}",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "npm run clean",
    "posttest": "npm run lint && npm run prettier:check",
    "prepublishOnly": "npm run build && oclif-dev manifest",
    "preversion": "npm run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "debug": "cross-env DEBUG=${DEBUG:-ghs:*} ./bin/run"
  },
  "types": "lib/index.d.ts"
}
